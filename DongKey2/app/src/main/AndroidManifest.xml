<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dongkey">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dongkey"
        android:label="동키"
        android:roundIcon="@drawable/dongkey"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SecondActivity" />
        <activity android:name=".ThirdActivity" />
        <activity android:name=".FourthActivity" />
        <activity android:name=".FifthActivity" />
        <activity android:name=".SixthActivity" />
        <activity android:name=".SeventhActivity" />
        <activity android:name=".EighthActivity" />
        <activity android:name=".NinethActivity" />
        <activity android:name=".TenthActivity" />
        <activity android:name=".EleventhActivity" />
        <activity android:name=".TwelvethActivity" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>